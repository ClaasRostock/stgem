{
  "sut": "mo3d.MO3D",
  "objective_func": [ "FalsifySTL", "FalsifySTL", "FalsifySTL"],

  "objective_func_parameters": [
    { "specification": "always[0,1] o0>0"},
    { "specification": "always[0,1] o1>0"},
    { "specification": "always[0,1] o2>0"}
  ],

  "objective_selector": "ObjectiveSelectorMAB",
  "objective_selector_parameters": {
    "warm_up": 30
  },
"job_parameters": {"max_tests": 80, "mode": "stop_at_first_objective"},
"steps": [
  {
  "algorithm": "random.Random",
  "algorithm_parameters": {
     "use_predefined_random_data": false,
     "predefined_random_data": {"test_inputs": null, "test_outputs": null}
    },
  "step_parameters": {"max_tests": 20}
  },
  {
"algorithm": "ogan.OGAN",
"algorithm_parameters":
    {"input_dimension": 3,
     "random_search_min_distance": 0.8,
     "use_predefined_random_data": false,
     "predefined_random_data": {"test_inputs": null, "test_outputs": null},
     "fitness_coef": 0.95,
     "train_delay": 1,
     "N_candidate_tests": 1,
     "ogan_model": "model.OGAN_Model",
     "ogan_model_parameters": {"optimizer": "Adam",
                               "discriminator_lr": 0.005,
                               "discriminator_betas": [0.9, 0.999],
                               "generator_lr": 0.0010,
                               "generator_betas": [0.9, 0.999],
                               "noise_batch_size": 512,
                               "generator_loss": "MSE",
                               "discriminator_loss": "MSE"},
     "train_settings_init": {"epochs": 2, "discriminator_epochs": 20, "generator_batch_size": 32},
     "train_settings": {"epochs": 1, "discriminator_epochs": 30, "generator_batch_size": 32},
     "generator_mlm": "GeneratorNetwork",
     "generator_mlm_parameters": {"noise_dim": 20, "output_shape": 3, "neurons": 64},
     "discriminator_mlm": "DiscriminatorNetwork",
     "discriminator_mlm_parameters": {"input_shape": 3, "neurons": 64, "discriminator_output_activation": "sigmoid"}
    },
"step_parameters": {"max_tests": 200, "N_random_init": 20, "mode": "stop_at_first_objective"}
  }
]
}



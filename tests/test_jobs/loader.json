{
"sut": "mo3d.MO3D",

"objective_func": ["Minimize", "Minimize", "Minimize"],
"objective_func_parameters": [{"selected": [0], "scale": true}, {"selected": [1], "scale": true}, {"selected": [2], "scale": true}],

"objective_selector": "ObjectiveSelectorMAB",
"objective_selector_parameters": {"warm_up": 30},

"steps": ["step_loader", "step_random_search", "step_ogan"],
"step_loader": {
  "step_parameters": {"max_tests": 10, "mode": "stop_at_first_objective"},
  "algorithm": "LoaderAlgorithm",
  "algorithm_parameters": {"data_file": "foo.npy"}
},
"step_random_search": {
  "step_parameters": {"max_tests": 10, "mode": "stop_at_first_objective"},
  "algorithm": "random.Random"
},
"step_ogan": {
  "step_parameters": {"max_tests": 60, "mode": "copy:step_random_search.step_parameters.mode"},
  "algorithm": "ogan.OGAN",
  "algorithm_parameters": {
    "random_search_min_distance": 0.8,
    "fitness_coef": 0.95,
    "train_delay": 1,
    "N_candidate_tests": 1,
    "ogan_model": "model.OGAN_Model",
    "ogan_model_parameters": {"optimizer": "Adam",
                              "discriminator_lr": 0.005,
                              "discriminator_betas": [0.9, 0.999],
                              "generator_lr": 0.0010,
                              "generator_betas": [0.9, 0.999],
                              "noise_batch_size": 512,
                              "generator_loss": "MSE",
                              "discriminator_loss": "MSE"},
    "train_settings_init": {"epochs": 2, "discriminator_epochs": 20, "generator_batch_size": 32},
    "train_settings": {"epochs": 1, "discriminator_epochs": 30, "generator_batch_size": 32},
    "generator_mlm": "GeneratorNetwork",
    "generator_mlm_parameters": {"noise_dim": 20, "output_shape": "copy:step_ogan.algorithm_parameters.input_dimension", "neurons": 64},
    "discriminator_mlm": "DiscriminatorNetwork",
    "discriminator_mlm_parameters": {"input_shape": "copy:step_ogan.algorithm_parameters.input_dimension", "neurons": 64, "discriminator_output_activation": "sigmoid"}
   }
}
}
